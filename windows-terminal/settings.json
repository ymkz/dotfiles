{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": "find",
      "keys": "ctrl+shift+f"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "right"
      },
      "keys": "alt+d"
    },
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+c"
    },
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": {
        "action": "newTab"
      },
      "keys": "alt+t"
    },
    {
      "command": "closePane",
      "keys": "alt+w"
    },
    {
      "command": {
        "action": "switchToTab",
        "index": 0
      },
      "keys": "alt+1"
    },
    {
      "command": {
        "action": "switchToTab",
        "index": 1
      },
      "keys": "alt+2"
    },
    {
      "command": {
        "action": "switchToTab",
        "index": 2
      },
      "keys": "alt+3"
    },
    {
      "command": {
        "action": "switchToTab",
        "index": 3
      },
      "keys": "alt+4"
    }
  ],
  "copyFormatting": "none",
  "copyOnSelect": false,
  "defaultProfile": "{2c4de342-38b7-51cf-b940-2309a097f518}",
  "disableAnimations": true,
